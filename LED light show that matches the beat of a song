}
  
void loop()
{
  while (Serial.available()==0){
  }
    int userinput = Serial.parseInt();
  
    switch (userinput)
    {
      case 1:
      LedLightShow1();
      break;
      
      case 2:
      LedLightShow2();
      break;
      
      case 3:
      LedLightShow3();
      break;
      
      default:
      Serial.println("Error");
   }
 }
