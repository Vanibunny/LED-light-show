void LedLightShow3()
{
  //GRADUAL ON-OFF//
  digitalWrite(13, HIGH); 
  delay(100);
  digitalWrite(12, HIGH);
  delay(100); 
  
  digitalWrite(13, LOW);
  delay(50); 
  digitalWrite(12, HIGH); 
  delay(100);
  digitalWrite(11, HIGH);
  delay(100);
  
  digitalWrite(12, LOW);
  delay(50); 
  digitalWrite(11, HIGH); 
  delay(100);
  digitalWrite(10, HIGH);
  delay(100);
  
  digitalWrite(11, LOW);
  delay(50); 
  digitalWrite(10, HIGH); 
  delay(100);
  digitalWrite(9, HIGH);
  delay(100);
  
  digitalWrite(10, LOW);
  delay(50); 
  digitalWrite(9, HIGH); 
  delay(100);
